                towers[towerUp].range *= 1.3;
                towers[towerUp].dmg *= 1.3;
                towers[towerUp].frztm *= 1.3;
                
                towers[towerUp].spendt += (towers[towerUp].upg * 0.75);
                towers[towerUp].upg *= 1.25;
                
                towers[towerUp].upgCnt++;
                
            }
            
